---
type Props = {
  title: string;
  emoji: string;
  createdAt: string;
  tags: string[];
};

import { MdDetail } from "@/components/MdDetail";

const { title, createdAt, emoji, tags } = Astro.props;
---

<MdDetail title={title} emoji={emoji} createdAt={createdAt} tags={tags}>
  <slot />
</MdDetail>
